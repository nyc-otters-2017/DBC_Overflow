<header>
  <h1>All Questions</h1>
</header>


<div class="">
  <ul>
    <%@questions.each do |question| %>
    <%=question.get_total_votes%> |
    <a href="# "><i class="tiny material-icons">thumb_up</i></a>
    <i class="tiny material-icons">thumb_down</i>
    <a href="/questions/<%=question.id%>"><li> <%= question.title %> </li></a>
    <% end %>
  </ul>
</div>
